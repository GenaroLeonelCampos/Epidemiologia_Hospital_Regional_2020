@model IEnumerable<Epidemiologia.ListaModelos.EmpleadosL>

@{
    ViewData["Title"] = "Index";
}

<ol class="breadcrumb  badge-dark">
    <table class="egt col-md-12">
        <tr>
            <td align="left"> <h2>Personal de salud</h2></td>
            @*<td align="right"><h1><a asp-action="exportarExcel" asp-controller="Responsable" class="btn btn-light"><i class="fa fa-print" aria-hidden="true"></i></a></h1></td>*@
            @*<td align="right"><h1><a asp-action="Agregar" class="btn btn-warning"><i class="fa fa-plus-square" aria-hidden="true"></i></a></h1></td>*@
        </tr>
    </table>
</ol>
<table class="table" id="table">
    <thead class="badge-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdEmpleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Completo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dni)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaIngreso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaNacimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoSexo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pais)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <th>
                @Html.DisplayFor(modelItem => item.IdEmpleado)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.Completo)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.Dni)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.FechaIngreso)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.FechaNacimiento)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.TipoSexo)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.Pais)
            </th>
        </tr>
        }
    </tbody>
</table>


<script src="~/js/generic.js"></script>
<script src="~/js/sweetalert.js"></script>