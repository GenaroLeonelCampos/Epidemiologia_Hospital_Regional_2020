@model Epidemiologia.ListaModelos.MedicamentoL

@{
    ViewData["Title"] = "Agregar";
}

<ol class="breadcrumb badge-dark">
    <table class="egt col-md-12">
        <tr>
            <td> <h1>Agregar Insumos- Materiales-Medicamentos</h1></td>
            <td align="right"><h1><a asp-action="Index" class="btn btn-success"><i class="fa fa-home" aria-hidden="true"></i></a></h1></td>
        </tr>
    </table>
</ol>
<br />
<div class="row">
    <div class="col-md-12">
        <form asp-action="Agregar">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="row">
                <div class="col-md-2">
                    <div class="form-group">
                        <label asp-for="Cod_sismed" class="control-label"></label>
                        <input asp-for="Cod_sismed" class="form-control" />
                        <span asp-validation-for="Cod_sismed" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="Denominacion" class="control-label"></label>
                        <input asp-for="Denominacion" class="form-control" />
                        <span asp-validation-for="Denominacion" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label asp-for="Concentracion" class="control-label"></label>
                        <input asp-for="Concentracion" class="form-control" />
                        <span asp-validation-for="Concentracion" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label asp-for="Presentacion" class="control-label"></label>
                        <input asp-for="Presentacion" class="form-control" />
                        <span asp-validation-for="Presentacion" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2">
                    <div class="form-group">
                        <label asp-for="Saldo" class="control-label"></label>
                        <input asp-for="Saldo" class="form-control" />
                        <span asp-validation-for="Saldo" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-10">
                    <div class="form-group">
                        <label asp-for="Observacion" class="control-label"></label>
                        <input asp-for="Observacion" class="form-control" />
                        <span asp-validation-for="Observacion" class="text-danger"></span>
                    </div>
                </div>
                <br />
                <div class="form-group">
                    <input type="submit" value="Guardar" class="btn btn-primary" />
                </div>

            </div>
        </form>
    </div>
</div>