@model IEnumerable<Epidemiologia.ListaModelos.MedicamentoL>

@{
    ViewData["Title"] = "Index";
}

<ol class="breadcrumb  badge-dark">
    <table class="egt col-md-12">
        <tr>
            <td align="left"> <h2>Listado de Insumos - Materiales - Medicamentos</h2></td>
            <td align="right"><h1><a asp-action="exportarExcel" asp-controller="Medicamento" class="btn btn-light"><i class="fa fa-print" aria-hidden="true"></i></a></h1></td>
            <td align="right"><h1><a asp-action="Agregar" class="btn btn-warning"><i class="fa fa-plus-square" aria-hidden="true"></i></a></h1></td>
        </tr>
    </table>
</ol>
<table class="table" id="table">
    <thead class="badge-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cod_sismed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Denominacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Concentracion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Presentacion)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.Ingresos)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.Saldo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_Ingreso)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Cod_sismed)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Denominacion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Concentracion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Presentacion)
                </td>
                @*<td>
                    @Html.DisplayFor(modelItem => item.Ingresos)
                </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.Saldo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Fecha_Ingreso)
                </td>
            </tr>
        }
    </tbody>
</table>
<script src="~/js/generic.js"></script>
<script src="~/js/sweetalert.js"></script>