@model IEnumerable<Epidemiologia.ListaModelos.SalidasL>

@{
    ViewData["Title"] = "Index";
}

<ol class="breadcrumb  badge-dark">
    <table class="egt col-md-12">
        <tr>
            <td align="left"> <h2>Salida de Insumos - Materiales - Medicamentos a Farmacia</h2></td>
            <td align="right"><h1><a asp-action="exportarExcel" asp-controller="Salida" class="btn btn-light"><i class="fa fa-print" aria-hidden="true"></i></a></h1></td>
            <td align="right"><h1><a asp-action="Agregar" class="btn btn-warning"><i class="fa fa-plus-square" aria-hidden="true"></i></a></h1></td>
        </tr>
    </table>
</ol>
<table class="table" id="table">
    <thead class="badge-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Medicamento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Responsable)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Servicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Recepcionante)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pertenencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaDistrib)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cantidad)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Medicamento)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Responsable)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Servicio)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Recepcionante)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pertenencia)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FechaDistrib)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cantidad)
                </td>
            </tr>
        }
    </tbody>
</table>
<script src="~/js/generic.js"></script>
<script src="~/js/sweetalert.js"></script>